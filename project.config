[project]
	description = MediaWiki extension ArticleRatings
[access]
	inheritFrom = mediawiki/extensions
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/*"]
	owner = group extension-ArticleRatings
[allowSuperproject "mediawiki/extensions"]
	matching = refs/heads/master:refs/heads/master
